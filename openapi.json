  "info": {
    "title": "CIPILAB API",
    "description": "API documentation for CIPILAB",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "LoginReqDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string"
          },
          "rememberMe": {
            "type": "boolean"
          }
        },
        "required": [
          "email",
          "password",
          "rememberMe"
        ]
      },
      "UserRole": {
        "type": "string",
        "enum": [
          "FULL_ADMIN",
          "SERVICES_ADMIN",
          "VIEWONLY_ADMIN",
          "USER"
        ]
      },
      "UserEntity": {
        "type": "object",
        "properties": {
          "role": {
            "allOf": [
              {
                "$ref": "#/components/schemas/UserRole"
              }
            ]
          },
          "id": {
            "type": "number"
          },
          "username": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "role",
          "id",
          "username",
          "email",
          "createdAt",
          "updatedAt"
        ]
      },
      "TokenRefreshResDto": {
        "type": "object",
        "properties": {
          "sessionId": {
            "type": "number"
          },
          "accessToken": {
            "type": "string"
          }
        },
        "required": [
          "sessionId",
          "accessToken"
        ]
      },
      "LoginResDto": {
        "type": "object",
        "properties": {
          "user": {
            "$ref": "#/components/schemas/UserEntity"
          },
          "session": {
            "$ref": "#/components/schemas/TokenRefreshResDto"
          }
        },
        "required": [
          "user",
          "session"
        ]
      },
      "TokenRefreshReqDto": {
        "type": "object",
        "properties": {
          "sessionId": {
            "type": "number"
          }
        },
        "required": [
          "sessionId"
        ]
      },
      "ChangePasswordReqDto": {
        "type": "object",
        "properties": {
          "currentPassword": {
            "type": "string"
          },
          "newPassword": {
            "type": "string",
            "minLength": 8,
            "maxLength": 128,
            "pattern": "/^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[\\W_]).+$/"
          }
        },
        "required": [
          "currentPassword",
          "newPassword"
        ]
      },
      "StatusResponseDto": {
        "type": "object",
        "properties": {
          "setupCompleted": {
            "type": "boolean"
          },
          "serverName": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/UserEntity"
          }
        },
        "required": [
          "setupCompleted"
        ]
      },
      "InitDashboardReqDto": {
        "type": "object",
        "properties": {
          "serverName": {
            "type": "string",
            "minLength": 3,
            "maxLength": 30
          },
          "username": {
            "type": "string",
            "minLength": 3,
            "maxLength": 30,
            "pattern": "/^[a-zA-Z0-9_]+$/"
          },
          "email": {
            "type": "string",
            "format": "email",
            "pattern": "/^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/"
          },
          "password": {
            "type": "string",
            "minLength": 8,
            "maxLength": 128,
            "pattern": "/^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[\\W_]).+$/"
          }
        },
        "required": [
          "serverName",
          "username",
          "email",
          "password"
        ]
      }
    }
  }
}